{
  "name": "sample-lib",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "main": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "esm"
  ],
  "scripts": {
    "clean": "rimraf dist/ esm/",
    "build": "pnpm clean && pnpm lint && pnpm run \"/^build:(?!analyze).*/\"",
    "build:analyze": "rollup --config --configAnalyze",
    "build:bundle": "rollup -c",
    "build:type": "tsc -p tsconfig.build.json",
    "watch": "rollup -c -w",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "prepack": "run-s build"
  },
  "devDependencies": {
    "@configs/babel": "workspace:*",
    "@configs/eslint-config": "workspace:^",
    "@configs/rollup": "workspace:*",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.48.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17 || ^18"
  },
  "publishConfig": {
    "type": "module",
    "exports": {
      ".": {
        "import": "./esm/index.js",
        "require": "./dist/index.cjs",
        "types": "./types/index.d.ts"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./esm/index.js",
    "types": "./types/index.d.ts"
  }
}
